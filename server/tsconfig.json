{
  "compilerOptions": {
    "target": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "CommonJS",
    "types": [
      "@citizenfx/server",
      "@types/node",
      "@overextended/ox_lib/server"
    ],
    "lib": ["ES2020"],
    "paths": {
      "#server/*": ["./*"]
    },
    "skipLibCheck": true
  },
  "include": ["./**/*"],
  "exclude": ["**/node_modules", "**/__tests__/*"]
}
